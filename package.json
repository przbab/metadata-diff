{
  "name": "metadata-diff",
  "version": "0.0.0",
  "main": "index.js",
  "bin": {
    "metadata-diff": "./bin/metadata-diff.js"
  },
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "precommit": "lint-staged",
    "prettier": "prettier --write",
    "start": "node index",
    "stylelint": "stylelint './**/*.scss' --syntax scss",
    "test": "npm run lint && npm run stylelint"
  },
  "license": "MIT",
  "lint-staged": {
    "*.js": [
